{
  "lock": "deno.lock",
  "imports": {
    "@/": "./src/",
    "dotenv": "https://deno.land/std@0.176.0/dotenv/mod.ts",
    "grammy": "https://deno.land/x/grammy@v1.14.1/mod.ts",
    "grammy-i18n": "https://deno.land/x/grammy_i18n@v1.0.1/mod.ts"
  },
  "fmt": {
    "files": {
      "include": [
        "src/"
      ]
    },
    "options": {
      "useTabs": false,
      "indentWidth": 2,
      "lineWidth": 88,
      "semiColons": true,
      "singleQuote": false
    }
  }
}
